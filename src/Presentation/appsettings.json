{
	"AllowedHosts": "*",
	"Authentication": {
		"Audience": "<audience>",
		"Issuer": "<issuer>",
		"SecurityKey": "<64chars>",
		"TokenDuration": 1209600000
	},
	"ConnectionStrings": {
		"DefaultConnection": "<database>"
	},
	"Database": {
		"UseInMemoryDatabase": false,
		"UseMigrationsEndPoint": false
	},
	"ImagePath": "<imageFolder>",
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"MaxUploadSize": 100000000,
	"VideoPath": "<videoFolder>"
}
