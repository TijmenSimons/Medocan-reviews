{
	"AllowedHosts": "*",
	"Authentication": {
		"Audience": "<Audience>",
		"Issuer": "<Issuer>",
		"SecurityKey": "<SecurityKey>",
		"TokenDuration": 600000
	},
	"ConnectionStrings": {
		"DefaultConnection": "<DefaultConnection>"
	},
	"Database": {
		"UseInMemoryDatabase": true,
		"UseMigrationsEndPoint": false
	},
	"Logging": {
		"LogLevel": {
			"Default": "Warning"
		}
	}
}
